{
  "name": "dubbo2-ts",
  "version": "0.0.1",
  "description": "dubbo for nodejs",
  "keywords": [
    "dubbojs"
  ],
  "author": "lileilei <lileilei@ewell.cc>",
  "homepage": "http://192.168.150.61:2080/Web-team/sh-emr-group/packages",
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "es6": "lib",
    "test": "__tests__"
  },
  "files": [
    "es6",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib es6 types coverage",
    "test": "node test.js",
    "build": "yarn clean && yarn build:tsc:es5  && yarn build:tsc:es6",
    "build:tsc:es5": "tsc --declarationDir ./lib/typings",
    "build:tsc:es6": "tsc  --project tsconfig.es6.json  --declarationDir ./es6/typings",
    "start": "npm-run-all -p start:tsc:es5 start:tsc:es6",
    "start:tsc:es5": "tsc  --watch --preserveWatchOutput",
    "start:tsc:es6": "tsc  --project tsconfig.es6.json  --watch --preserveWatchOutput"
  },
  "publishConfig": {
    "registry": "http://192.168.150.59:8081/repository/npm-host1"
  },
  "repository": {
    "type": "git",
    "url": "http://192.168.150.61:2080/Web-team/sh-emr-group/plugins/dubbo2-ts"
  },
  "bugs": {
    "url": "http://192.168.150.61:2080/Web-team/sh-emr-group/issues"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "hessian.js": "^2.10.0",
    "interpret-util": "^0.0.6",
    "ip": "^1.1.5",
    "js-to-java": "^2.6.1",
    "koa-compose": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "^6.6.2",
    "urllib": "^2.36.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/ip": "^1.1.0"
  }
}
